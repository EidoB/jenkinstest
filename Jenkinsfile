            sh 'echo fetching'
            }
        }
          stage('building'){
            steps{
            sh 'echo building'
            }
        }
          stage('testing'){
            steps{
            sh 'echo testing'
            }
        }
          stage('deploying'){
            steps{
            echo 'deploying'
            }
        }
    }
}